{
  "experiment_id": "experiment_p0_p5_o4mini",
  "random_seed": 42,
  "runs_per_item": 1,
  "prompts": [
    {
      "id": "P0_baseline",
      "template_path": "p0_baseline.j2",
      "description": "Single-pass baseline with JSON output format"
    },
    {
      "id": "P1_icl",
      "template_path": "p1_icl_single_type.j2",
      "description": "Per-vulnerability-type ICL with 10 API calls"
    },
    {
      "id": "P2_structured",
      "template_path": "p2_structured_reasoning.j2",
      "description": "Chain-of-thought with systematic audit checklist"
    },
    {
      "id": "P3_smartguard",
      "template_path": "p3_smartguard.j2",
      "description": "RAG-enhanced with pattern retrieval and self-check"
    },
    {
      "id": "P4_tool_augmented",
      "template_path": "p4_stage1_scenario.j2",
      "description": "GPTScan-aligned: pre-filter + 3-stage LLM + static confirmation"
    },
    {
      "id": "P5_smartaudit",
      "template_path": "p5_project_manager.j2",
      "description": "LLM-SmartAudit multi-agent with 4 specialized agents"
    }
  ],
  "models": [
    {
      "name": "o4-mini",
      "provider": "openai",
      "params": {
        "base_url": "https://novaimsplayground.cognitiveservices.azure.com/openai/v1/",
        "api_key_env": "AZURE_OPENAI_API_KEY",
        "max_tokens": 65536
      }
    }
  ],
  "datasets": [
    {
      "name": "smartbugs_curated",
      "kind": "smartbugs_curated",
      "path": "data/smartbugs_curated/dataset"
    }
  ],
  "output_dir": "outputs/experiment_v1",
  "notes": "P0-P5 prompt strategies with o4-mini reasoning model.\nP6 runs separately on dah2 server with fine-tuned CodeLlama models.\nEstimated cost: ~$62\n",
  "tools": {
    "top_k_patterns": 5,
    "enable_cot_expansion": true,
    "use_icl_template": true,
    "enable_iterative_selfcheck": true,
    "selfcheck_max_iterations": 3,
    "selfcheck_convergence_threshold": 0.9,
    "slither_timeout": 120,
    "enable_static_confirmation": true,
    "p4_stage1_template": "p4_stage1_scenario.j2",
    "p4_stage2_template": "p4_stage2_property.j2",
    "p4_stage3_template": "p4_stage3_extraction.j2",
    "slither_mode": "confirmation_only",
    "enable_reachability_filter": true,
    "smartaudit_mode": "BA",
    "smartaudit_max_rounds": 3,
    "smartaudit_consensus_threshold": 0.9,
    "smartaudit_enable_role_exchange": true,
    "smartaudit_scenarios": null,
    "smartaudit_scenario_path": null,
    "p6_iaudit_detector_model": "",
    "p6_iaudit_reasoner_model": "",
    "p6_iaudit_ranker_model": "mistralai/Mixtral-8x7B-Instruct-v0.1",
    "p6_iaudit_use_4bit": true,
    "p6_iaudit_max_ranker_rounds": 5,
    "p6_iaudit_detector_prompts": 5,
    "p6_iaudit_reasoner_prompts": 10,
    "p6_iaudit_hf_token": ""
  }
}